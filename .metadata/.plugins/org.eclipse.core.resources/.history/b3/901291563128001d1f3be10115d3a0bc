package UstDemo;

public class Table {

	public static void main(String[] args) {
		int i,j,k;
		System.out.print("Multiplication Table of 24")
		for(i=1;i<=10;i++)
		{
			int m=24*i;
			System.out.println(i+" * 24 = "+m);
		}
		for(j=1;j<=10;j++)
		{
			int n=24*j;
			System.out.println(j+" * 50 = "+n);
		}
		for(k=1;k<=10;k++)
		{
			int p=24*k;
			System.out.println(k+" * 50 = "+p);
		}

	}

}
