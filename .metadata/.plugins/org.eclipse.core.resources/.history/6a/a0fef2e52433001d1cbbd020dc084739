package Sorting;

public class SelectionSort {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int [] a = {17,21,10,7,34,19};
		int i;
		System.out.println("Numbers Before Sorting");
		for(i=0;i<a.length;i++) 
		{
			System.out.print(a[i]+" ");
		}
		System.out.println();

	}

}
